{
  "name": "begat-game",
  "version": "1.0.0",
  "description": "Biblical genealogy trivia/puzzle game",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint": "eslint js/**/*.js tests/**/*.js",
    "lint:fix": "eslint js/**/*.js tests/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,css,html,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,css,html,md,json}\"",
    "serve": "npx serve ."
  },
  "devDependencies": {
    "@testing-library/dom": "^10.0.0",
    "cypress": "^13.6.0",
    "eslint": "^8.56.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "bible",
    "game",
    "genealogy",
    "trivia",
    "educational"
  ],
  "author": "GRANDPAPA.NET",
  "license": "UNLICENSED",
  "private": true
}
